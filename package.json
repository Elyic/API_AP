{
  "name": "noapirest",
  "version": "1.0.0",
  "description": "Servicio para noapirest",
  "main": "index.js",
  "scripts": {
    "copyAll": "shx cp -r src/static build/static && shx cp -r src/views build/views && shx cp -r src/uploads build/uploads",
    "test": "echo \"Error: no test specified\" && exit 1",
    "html": "copyfiles --up 1 src/public/*.html dist ",
    "build": "tsc && npm run html",
    "start": "node dist/src/index.js",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "postinstall": "ng build --aot",
    "heroku-prebuild": "export NPM_CONFIG_PRODUCTION=false; export NODE_ENV=; NPM_CONFIG_PRODUCTION=false NODE_ENV=development npm install --only=dev --dev",
    "heroku-postbuild": "export NPM_CONFIG_PRODUCTION=true; export NODE_ENV=production; npm run build"
  },
  "author": "Harold Cordon",
  "license": "ISC",
  "private": true,
  "dependencies": {
    "@types/mssql": "^4.0.12",
    "copyfiles": "^2.1.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "fstream": "^1.0.12",
    "i": "^0.3.6",
    "mssql": "^5.0.0",
    "npm": "^6.9.0",
    "tar": "^4.4.8",
    "typescript": "^2.4.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.1",
    "typescript": "^2.4.0",
    "shx": "^0.3.2"
  },
  "engines": {
    "node": "10.15.3",
    "npm": "^6.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Elyic/API_AP.git"
  }
}
